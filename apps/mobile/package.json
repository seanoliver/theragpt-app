{
  "name": "mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "dev": "expo start",
    "build": "expo export --platform android --platform ios",
    "lint": "eslint .",
    "clean": "rm -rf .expo .turbo node_modules",
    "async:clear:ios": "xcrun simctl get_app_container booted com.helloseanoliver.still data | xargs -I {} rm -rf {}/Library/Caches/{,RCTAsyncLocalStorage_V1} {}/Library/Application\\ Support/{,RCTAsyncLocalStorage_V1}",
    "async:clear:android": "adb shell pm clear com.helloseanoliver.still",
    "async:clear": "pnpm async:clear:ios && pnpm async:clear:android"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.9",
    "@expo-google-fonts/inter": "^0.3.0",
    "@expo-google-fonts/playfair-display": "^0.3.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@still/config": "workspace:*",
    "@still/logic": "workspace:*",
    "expensify-common": "^2.0.129",
    "expo": "~52.0.46",
    "expo-constants": "^17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "^13.0.4",
    "expo-linear-gradient": "^14.0.2",
    "expo-linking": "^7.0.5",
    "expo-router": "~4.0.20",
    "expo-status-bar": "~2.0.1",
    "lodash": "^4.17.21",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-get-random-values": "^1.11.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-keyboard-controller": "^1.17.0",
    "react-native-modal": "14.0.0-rc.1",
    "react-native-pager-view": "6.5.1",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-native-community/cli": "^18.0.0",
    "@types/lodash": "^4.17.16"
  },
  "private": true
}
